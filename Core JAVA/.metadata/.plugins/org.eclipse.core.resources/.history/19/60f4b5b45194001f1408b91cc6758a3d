package com.math;

public class Test {

	public static void main(String[] args) {
		
		
		System.out.println("Maximim Number is :"+Math.max(51, 10)); //return max number
		
		System.out.println("Minimum Number is :"+Math.min(52, 10)); //return minimum number
		
		System.out.println("Square root of X:"+Math.sqrt(64)); //return square-root 
		
		 System.out.println("Rounf Off Number :"+Math.abs(-4.7)); // return whole number
		 
		 System.out.println("Generate Random Number :"+Math.random()); //Generate Random Number
		 
		 
		 
		 
		 int randomNum = (int)(Math.random() * 101);  // 0 to 100
		    System.out.println("Random number Between 100"+randomNum);
		

	}

}
